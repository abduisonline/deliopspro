            function getBookingsAwaitingAccept() {
                // AJAX request
                var bookingid = 0;
                var url = "https://classfleet.rapidfiresol.com/admin/getBookingawaitingaccept/:bookingid";
                url = url.replace(':bookingid',bookingid);

                // Empty modal data
                $('#tblbookinginfo tbody').empty();

                $.ajax({
                    url: url,
                    dataType: 'json',
                    success: function(response){
                        if(response.html.length){
                            $('#tblbookinginfo tbody').html(response.html);
                            $('#bookingawaitingaccept').modal('show'); 
                        }
                    }
                });
            }

            // Call the function every 10 seconds
            var businessID = 3;
            
            if(businessID == 1){
                setInterval(getBookingsAwaitingAccept, 600000); // 10000 milliseconds = 10 seconds
            }

